5. CREATE A TABLE emp WITH emp_id, name,basic
    create table emp (emp_id int, name varchar2(25), basic number);

  5.1 Insert 10 rows
    insert into emp values (1043, 'Mohammed Zaid Shaikh', 45000);
  5.2 Add hra, da, total
    alter table emp add(hra number, da number, total number);
  5.3 Compute hra as 7.5% of basic
    update emp set hra = 0.075 * basic;
  5.4 Compute da as 150% of basic
    update emp set da = 1.5 * basic;
  5.5 Compute total
    update emp set total = hra + da;
  5.6 Add dedu, take_home
    alter table emp add(dedu number, take_home number);
  5.7 Compute dedu as 12% of total
    update emp set dedu = 0.12 * basic;
  5.8 Compute take_home as difference between dedu and total
    update emp set take_home = total - dedu;
